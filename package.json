{
  "name": "ssh-tool-new-electron",
  "productName": "ssh-tool-new-electron",
  "version": "1.0.0",
  "description": "Local offline tools for developers",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Darragh ORiordan",
    "email": "darragh.oriordan@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ssh_tool_new_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/public/index.html",
                  "js": "./src/app/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/electron/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "6.0.0-beta.65",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/electron-devtools-installer": "2.2.2",
    "@types/git-url-parse": "9.0.1",
    "@types/ini": "1.3.31",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.14",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "20.1.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "29.0.1",
    "node-loader": "^2.0.0",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "style-loader": "^3.0.0",
    "tailwindcss": "3.1.8",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "2.0.8",
    "@tailwindcss/forms": "0.5.2",
    "autoprefixer": "10.4.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "electron-squirrel-startup": "1.0.0",
    "file-loader": "^6.2.0",
    "git-url-parse": "12.0.0",
    "ini": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0"
  }
}
